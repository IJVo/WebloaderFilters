extensions:
  webloader: WebLoader\Nette\Extension

services:
  jsMin: Joseki\Webloader\JsMinFilter

webloader:
  jsDefaults:
    sourceDir: %appDir%/files
    tempDir: %appDir%/../../temp
  js:
    default:
      files:
        - example.js
      filters:
        - @jsMin
    file:
      files:
        - example.min.js
        - example.js
      fileFilters:
        - @jsMin
